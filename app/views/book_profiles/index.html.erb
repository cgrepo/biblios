<p id="notice"><%= notice %></p>

<h1>Listing Book Profiles</h1>

<table>
  <thead>
    <tr>
      <th>Published</th>
      <th>Isbn</th>
      <th>Title</th>
      <th>Autor</th>
      <th>classification</th>
      <th>Label</th>
      <th>Book</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @book_profiles.each do |book_profile| %>
      <tr>
        <td><%= book_profile.published %></td>
        <td><%= book_profile.isbn %></td>
        <td><%= book_profile.title %></td>
        <td><%= book_profile.autor %></td>
        <td><%= book_profile.classification %></td>
        <td><%= book_profile.label %></td>
        <td><%= book_profile.book %></td>
        <td><%= link_to 'Show', book_profile %></td>
        <td><%= link_to 'Edit', edit_book_profile_path(book_profile) %></td>
        <td><%= link_to 'Destroy', book_profile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Book profile', new_book_profile_path %>
