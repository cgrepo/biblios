<p id="notice"><%= notice %></p>

<h1>Listing Books</h1>

<table>
  <thead>
    <tr>
      <th>Entrydate</th>
      <th>Copy</th>
      <th>Volumn</th>
      <th>Tome</th>
      <th>Accessible</th>
      <th>Profile</th>
      <th>Library</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @books.each do |book| %>
      <tr>
        <td><%= book.entryDate %></td>
        <td><%= book.copy %></td>
        <td><%= book.volumn %></td>
        <td><%= book.tome %></td>
        <td><%= book.accessible %></td>
        <td><%= book.profile %></td>
        <td><%= book.library %></td>
        <td><%= link_to 'Show', book %></td>
        <td><%= link_to 'Edit', edit_book_path(book) %></td>
        <td><%= link_to 'Destroy', book, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Book', new_book_path %>
